{
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint": "run-s prettier eslint",
    "lint:fix": "run-s package:fix prettier:fix eslint:fix",
    "package:fix": "format-package -w",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "update": "yarn run npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,yaml}": "prettier --write"
  },
  "dependencies": {
    "@types/d3": "^6.3.0",
    "d3": "^6.7.0",
    "next": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "format-package": "^6.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-check-updates": "^11.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
